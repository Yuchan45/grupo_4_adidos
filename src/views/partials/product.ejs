<div class="product-list-container">
    <div class="main-item-container">
        <% for(let i=0; i<sneakers.length; i++) { %> 
      <div class="main-item">
        <div class="item-image">
           <a href="/product-details">
          <img
            src="<%= sneakers[i].image %> "
            alt=""
        /></a> 
        </div>
        <div class="prod-description">
            <h3 class="prod-category"><%= sneakers[i].category %></h3>
            <h2 class="prod-title"><%= sneakers[i].brand %></h2>
            <h2 class="prod-model"><%= sneakers[i].model %></h2>
            <h2 class="prod-price">ARS $<%- sneakers[i].price %></h2>
        </div>
    </div>
    <% } %> 
</div>
    